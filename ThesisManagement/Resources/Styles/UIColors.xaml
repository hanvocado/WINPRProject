<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="primary">#0A426E</Color>
    <Color x:Key="secondary">#1E74B2</Color>
    <SolidColorBrush x:Key="primaryBrush">#0A426E</SolidColorBrush>
    <SolidColorBrush x:Key="secondaryBrush">#1E74B2</SolidColorBrush>

    <!--Button Color-->
    <SolidColorBrush x:Key = "createNomal" Color = "#0A426E" />
    <SolidColorBrush x:Key = "createHover" Color = "#1E74B2" />
    <SolidColorBrush x:Key = "updateNomal" Color = "orange" />
    <SolidColorBrush x:Key = "updateHover" Color = "yellow" />
    <SolidColorBrush x:Key = "deleteNomal" Color = "red" />
    <SolidColorBrush x:Key = "deleteHover" Color = "#FF8080" />
    <SolidColorBrush x:Key = "detailNomal" Color = "Gray" />
    <SolidColorBrush x:Key = "detailHover" Color = "lightgray" />
    <SolidColorBrush x:Key = "searchNomal" Color = "DeepSkyBlue" />
    <SolidColorBrush x:Key = "searchHover" Color = "LightSkyBlue" />

    <!--Font Colors-->
    <SolidColorBrush x:Key = "titleColor" Color = "#BCBEE0" />
    <SolidColorBrush x:Key = "plainTextColor" Color = "#F1F1F1" />  
    
</ResourceDictionary>