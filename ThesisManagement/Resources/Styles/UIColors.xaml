<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="primary">#0A426E</Color>
    <Color x:Key="secondary">#0B60B0</Color>
    
    <SolidColorBrush x:Key="primaryBrush">#0A426E</SolidColorBrush>
    <SolidColorBrush x:Key="secondaryBrush">#0B60B0</SolidColorBrush>

    <SolidColorBrush x:Key="danger" Color="#b10000" />
    <SolidColorBrush x:Key="dangerHover" Color="#d70000" />

    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor" Color = "#BCBEE0" />
    <SolidColorBrush x:Key="plainTextColor" Color = "#F1F1F1" />  
    
</ResourceDictionary>